<!-- sidebar.fxml -->
<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="733.0" prefWidth="260.0" style="-fx-background-color: #fc6c00" AnchorPane.leftAnchor="0.0"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.SideBarController">
    <children>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="55.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/images/ziyara.png" />
            </image>
        </ImageView>
        <!-- Sidebar -->
        <VBox layoutX="93.0" layoutY="330.0" spacing="10">
            <!-- Sidebar Items -->
            <Button prefHeight="25.0" prefWidth="75.0" text="Users" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Transports" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Guides" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Boutiques" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Packs" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Visites" onAction="#handleVisitesButtonClicked" />
            <Button prefHeight="25.0" prefWidth="75.0" text="Reservation-Visites" onAction="#handleReservationVisitesButtonClicked" />
        </VBox>
    </children>
</AnchorPane>
